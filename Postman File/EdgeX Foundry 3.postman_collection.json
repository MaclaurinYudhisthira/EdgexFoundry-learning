				{
					"name": "create device profile",
					"request": {
						"method": "POST",
						"header": [],
						"body": {
							"mode": "raw",
							"raw": "[\r\n    {\r\n        \"apiVersion\": \"v2\",\r\n        \"profile\": {\r\n            \"name\": \"Raspberry-pi-fusion\",\r\n            \"manufacturer\": \"Rpi\",\r\n            \"model\": \"4B\",\r\n            \"labels\": [\r\n                \"rpi-1\"\r\n            ],\r\n            \"description\": \"Raspberry pi device\",\r\n            \"deviceResources\": [\r\n                {\r\n                    \"name\": \"Camera\",\r\n                    \"description\": \"Counts people within vible area\",\r\n                    \"properties\": {\r\n                        \"valueType\": \"Int64\",\r\n                        \"readWrite\": \"RW\",\r\n                        \"defaultValue\": \"On\",\r\n                        \"units\": \"\"\r\n                    }\r\n                },\r\n                {\r\n                    \"name\": \"LDR\",\r\n                    \"description\": \"Measures intensity of life\",\r\n                    \"properties\": {\r\n                        \"valueType\": \"Int64\",\r\n                        \"readWrite\": \"RW\",\r\n                        \"defaultValue\": \"On\",\r\n                        \"units\": \"\"\r\n                    }\r\n                }\r\n            ]\r\n        }\r\n    }\r\n]",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "http://{{edgex-ip}}:{{edgex-metadata}}/api/v2/deviceprofile",
							"protocol": "http",
							"host": [
								"{{edgex-ip}}"
							],
							"port": "{{edgex-metadata}}",
							"path": [
								"api",
